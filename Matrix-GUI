local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Matrix", "BloodTheme")
    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")


    MainSection:NewButton("Fly", "Makes You Fly", function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Nicuse/RobloxScripts/main/BypassedFly.lua"))() 
        Fly(true)
    end)

   MainSection:NewButton("Click TP", "Ctrl + Click", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Modmailuser/Teleport/main/Teleport"))()
    end)

-- Aimlock
    local Lock = Window:NewTab("Aimlock")
    local LockSection = Lock:NewSection("AimLock")

    LockSection:NewButton("Aimlock", "Q to lock onto player", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Modmailuser/Aimlock1/main/Aimlock"))()
    end)

    local Other = Window:NewTab("Other")
    local OtherSection = Other:NewSection("Other")

        OtherSection:NewButton("Faded", "Loads Faded", function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/Modmailuser/Faded-gui/main/Faded%20Gui'),true))()
    end)

    MainSection:NewButton("Client Sided Headless And Korblox", "Client Sided", function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/Modmailuser/HeadlessNdKorblox/main/HeadlessNdKorblox'),true))()
    end)

        LockSection:NewButton("CamLock", "Q to lock onto player", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Modmailuser/Cam-lock21/main/Camlock21"))()
    end)

       MainSection:NewButton("Free Animation Pack", "Free", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Modmailuser/Animation-Pack/main/a"))()
    end)

----- God
        local God = Window:NewTab("GodStuff")
    local GodSection = God:NewSection("GodStuff")

   GodSection:NewButton("GodMode UseLess", "Cannot Die", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Modmailuser/God/main/God"))()
    end)

        local Fun Stuff = Window:NewTab("Fun Stuff Coming Soon")
    local Fun StuffSection = Fun Stuff:NewSection("Fun Stuff Coming Soon")
